<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Game Keeper</h1>
    <p class="lead">roll the dice and get board.</p>
    <form class="create-form">
    <label for="Input">Search Board Games:</label>
  	<input id="search-word" type="text"  name="search-word">
  	<button type="submit" id="searchBtn">Submit</button>
  </form>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-sm">
        <ol class="list-group list-group-flush">
            <!--searched games go here-->
        </ol>
    </div>
    <div class="col-sm">
      <h3>Wishlist</h3>
      <ul >
          <li>Test:wishlists cards will go here</li>
        {{#each game}}
           {{#unless own }}
                {{> partials/card owned=false}}
            {{/unless}}
        {{/each}}
      </ul>
    </div>
    <div class="col-sm">
      <h3>My Games</h3>
      <ul>
          <li>Test: Games owned cards go here</li>
        {{#each game}}
            {{#if own}}
                {{> partials/card owned=true}}
            {{/if}}
        {{/each}}
      </ul>
    </div>
  </div>
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
