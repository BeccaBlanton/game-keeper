<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Game Keeper</h1>
    <p class="lead">roll the dice and get board.</p>
    <form class="create-form">
      <label for="Input">Search Board Games:</label>
      <input id="search-word" type="text" name="search-word" placeholder="Search...">
      <button type="submit" id="searchBtn">Submit</button>
      <br>
      <div class="suggestions-drop">
        <ul class="suggestions">
        </ul>
      </div>
    </form>
  </div>
</div>
<div class="container-fluid">
    <div class="row filter-by">
        <div class="col-2">
            <h3>Browse by category</h3>
            <div class="dropdown">
            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton"
                data-bs-toggle="dropdown" aria-expanded="false">
                Filter by:
            </button>
            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <li><a class="dropdown-item" href="#">Rating</a></li>
                <li><a class="dropdown-item" href="#">Play time</a></li>
                <li><a class="dropdown-item" href="#">Age</a></li>
            </ul>
            </div>
        </div>

        <div class="col-5">
            <h2 class="welcomeTitles">Wishlist</h2>
            <ul class="wishlist list-group d-flex flex-wrap justify-content-center">

                {{#each game}}
                {{> partials/card}}
                {{/each}}
            </ul>
        </div>

        <div class="col-5">
            <h2 class="welcomeTitles">My Games</h2>
            <ul class="ownlist list-group d-flex flex-wrap justify-content-center">

                {{#each game}}
                {{> partials/card}}
                {{/each}}
            </ul>
        </div>



